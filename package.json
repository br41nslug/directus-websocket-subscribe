		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@directus/types": "^10.0.0",
		"get-random-values": "^2.1.0",
		"nanoid": "^4.0.2",
		"ws": "^8.13.0"
	},
	"repository": {
		"type": "git",
