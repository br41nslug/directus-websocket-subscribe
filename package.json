{
	"name": "directus-extension-websocket-subscribe",
	"version": "3.1.12",
	"author": "brainslug",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook",
		"directus-subscribe",
		"directus-websocket"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.mjs",
		"source": "src/index.ts",
